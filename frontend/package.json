{
  "name": "cloud-storage-dashboard",
  "version": "0.1.0",
  "description": "A modular Svelte dashboard component for cloud storage visualization",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run package",
    "package": "svelte-package -o dist",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^4.0.5",
    "@types/node": "^20.11.0",
    "fast-check": "^3.15.0",
    "jsdom": "^24.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "keywords": [
    "svelte",
    "dashboard",
    "cloud-storage",
    "visualization",
    "component"
  ],
  "license": "MIT"
}
